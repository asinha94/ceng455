#include "commands.h"

_queue_id openR(uint16_t queue_id) {
	// Init Rx message queue
		_queue_id rx_queue_id = _msgq_open(READ_QUEUE_HANDLER_ID, 0);

		if (rx_queue_id == 0) {
		 printf("\nCould not open the server message queue\n");
		 _task_block();
		}

}


